# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

# Default settings for all files
[*]
end_of_line = lf
insert_final_newline = true
charset = utf-8

# Specific settings for TypeScript and JavaScript files
[*.ts]
indent_style = space
indent_size = 4

[*.js]
indent_style = space
indent_size = 4
